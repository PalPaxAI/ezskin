{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/battles",
      "destination": "/modes/battles.html"
    },
    {
      "source": "/upgrader",
      "destination": "/modes/upgrader.html"
    },
    {
      "source": "/roulette",
      "destination": "/modes/roulette.html"
    },
    {
      "source": "/crash",
      "destination": "/modes/crash.html"
    },
    {
      "source": "/jackpot",
      "destination": "/modes/jackpot.html"
    },
    {
      "source": "/coinflip",
      "destination": "/modes/coinflip.html"
    },
    {
      "source": "/saper",
      "destination": "/modes/saper.html"
    },
    {
      "source": "/redline",
      "destination": "/cases/redline.html"
    },
    {
      "source": "/luckylore",
      "destination": "/cases/luckylore.html"
    },
    {
      "source": "/blaze",
      "destination": "/cases/blaze.html"
    },
    {
      "source": "/tigertooth",
      "destination": "/cases/tigertooth.html"
    },
    {
      "source": "/deserteagle",
      "destination": "/cases/deserteagle.html"
    },
    {
      "source": "/ak47",
      "destination": "/cases/ak47.html"
    },
    {
      "source": "/m4a4",
      "destination": "/cases/m4a4.html"
    },
    {
      "source": "/awp",
      "destination": "/cases/awp.html"
    },
    {
      "source": "/1profit",
      "destination": "/cases/1profit.html"
    },
    {
      "source": "/poseidon",
      "destination": "/cases/poseidon.html"
    },
    {
      "source": "/25knife",
      "destination": "/cases/25knife.html"
    },
    {
      "source": "/10k",
      "destination": "/cases/10k.html"
    },
    {
      "source": "/profile",
      "destination": "/diff/profile.html"
    },
    {
      "source": "/deposit",
      "destination": "/diff/deposit.html"
    },
    {
      "source": "/terms",
      "destination": "/diff/terms.html"
    },
    {
      "source": "/privacy",
      "destination": "/diff/privacy.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

